  "bugs": {
    "url": "https://github.com/zilurrane/lifegram-api/issues"
  },
  "homepage": "https://github.com/zilurrane/lifegram-api#readme",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.18.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "dotenv": "16.0.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.4"
  }
}
