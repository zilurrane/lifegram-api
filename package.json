{
  "name": "lifegram-api",
  "version": "0.0.1",
  "description": "API Server for LifeGram App",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config src/index.ts",
    "build": "tsc",
    "start": "node -r dotenv/config build/index.js",
    "deploy-prod": "npm install && npm run build && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zilurrane/lifegram-api.git"
  },
